<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="LoginButton">
        <item name="android:textColor"> @color/colorClassWorkBackground </item>
        <item name="android:textSize"> 16dp </item>
        <item name="android:layout_marginEnd">@dimen/calsswork2_start_end_padding </item>
        <item name="android:layout_marginStart">@dimen/calsswork2_start_end_padding </item>
    </style>>

    <style name="DzButton">
    <item name="android:layout_width">match_parent</item>
     <item name="android:layout_height">match_parent</item>
    </style>

    <style name="ClassworkButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_gravity">end</item>
        <item name="android:layout_marginEnd">20dp</item>
    </style>

    <style name="TableRowMainActivity">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:gravity">start</item>

    </style>

</resources>
